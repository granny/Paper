     protected int adjustedTickDelay(int adjustment) {
         return this.requiresUpdateEveryTick() ? adjustment : reducedTickDelay(adjustment);
     }
@@ -62,7 +73,19 @@ public abstract class Goal {
         return (ServerLevel)level;

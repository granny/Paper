+    // Paper start - Mob Goal API
+    public boolean hasFlag(final Goal.Flag flag) {
+        return this.flags.contains(flag);
+    }
+
+    public void addFlag(final Goal.Flag flag) {
+        this.flags.add(flag);
+    }
